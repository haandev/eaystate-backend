{
  "name": "korri-starter",
  "version": "1.0.0",
  "repository": "https://github.com/haandev/korri-starter.git",
  "author": "haandev",
  "scripts": {
    "pre-setup": "npx rimraf ./node_modules/@ooic && npx rimraf ./yarn.lock",
    "setup": "npm run pre-setup && yarn",
    "start": "node build/index.js",
    "start:build": "npm run build && npm run start",
    "copy-core": "npx rimraf ./node_modules/@ooic && npx copy-folder-util npm-local/@ooic node_modules/@ooic  --quiet",
    "dev": "npm run copy-core && npx rimraf ./build-debug && node node_modules/@ooic/scripts/dev.js",
    "debug": "npm run dev",
    "build": "npm run copy-core && node node_modules/@ooic/scripts/build.js",
    "build-debug": "npm run copy-core && node node_modules/@ooic/scripts/build-debug.js",
    "servant": "npm-local/@ooic/servant/index.js"
  },
  "license": "MIT",
  "dependencies": {
    "@ooic/core": "./npm-local/@ooic/core",
    "@ooic/router": "./npm-local/@ooic/router",
    "@ooic/scripts": "./npm-local/@ooic/scripts",
    "@ooic/utils": "./npm-local/@ooic/utils",
    "@types/chalk": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.0.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.0",
    "jsonwebtoken": "^8.5.1",
    "uid2": "^1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/uid2": "^0.0.0",
    "copy-folder-util": "^0.2.5",
    "fs-extra": "^11.1.0"
  }
}
